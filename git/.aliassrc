# Navigation typo catchers
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias ......='cd ../../../../..'
alias cd.='cd ..'
alias cd..='cd ..'
alias cd...='cd ../../'
alias cd....='cd ../../../'
alias cd.....='cd ../../../../'

# NPM
alias nrd="npm run dev"
alias nrbd="npm run build:dev"
alias nrb="npm run build"

# GIT 
alias gcn='git clone'
alias gb='git branch'
alias gck='git checkout'
alias gcb='git checkout -b'
alias gs='git status'
alias ga='git add'
alias gaa='git add .'
alias gcm='git commit -m'
alias gps='git push'
alias gpl='git pull'
alias glog='git log --oneline --decorate'